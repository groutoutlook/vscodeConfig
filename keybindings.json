    "when": "inKeybindings && keybindingFocus"
  },
  {
    "key": "ctrl+[",
    "command": "-editor.action.outdentLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+]",
    "command": "editor.action.jumpToBracket",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+\\",
    "command": "-editor.action.jumpToBracket",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+]",
    "command": "-editor.action.indentLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+w ctrl+shift+q",
    "command": "workbench.action.closeOtherEditors"
  },
  {
    "key": "ctrl+numpad_add",
    "command": "-workbench.action.zoomIn"
  },
  // Binding for previewing a VapourSynth filterchain using vs-preview
  {
    "key": "F5",
    "command": "workbench.action.terminal.sendSequence",
    "args": {
      "text": "python -m vspreview \"${file}\"\u000D"
    },
    "when": "resourceExtname == '.vpy'"
  },
  {
    "key": "ctrl+shift+, ctrl+shift+k",
    "command": "workbench.action.openGlobalKeybindingsFile"
  },
  {
    "key": "ctrl+shift+, ctrl+shift+,",
    "command": "workbench.action.openSettingsJson"
  },
  {
    "key": "ctrl+w ctrl+shift+\\",
    "command": "workbench.action.duplicateActiveEditorGroupRight"
  },
  {
    "key": "ctrl+w ctrl+\\",
    "command": "workbench.action.duplicateActiveEditorGroupDown"
  },
  {
