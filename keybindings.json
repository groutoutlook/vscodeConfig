// Place your key bindings in this file to override the defaultsauto[]
// "-" is the disabled (default) keymap.
[
  {
    "key": "ctrl+w",
    "command": "-workbench.action.closeGroup",
    "when": "activeEditorGroupEmpty && multipleEditorGroups"
  },
  {
    "key": "ctrl+w",
    "command": "-workbench.action.terminal.killEditor",
    "when": "terminalEditorFocus && terminalFocus && terminalHasBeenCreated || terminalEditorFocus && terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+w",
    "command": "-workbench.action.closeActiveEditor"
  },
  {
    "key": "ctrl+shift+alt+t",
    "command": "workbench.trust.manage"
  },
  {
    "key": "ctrl+shift+f4",
    "command": "workbench.action.closeWindow"
  },
  {
    "key": "ctrl+shift+w",
    "command": "-workbench.action.closeWindow"
  },
  {
    "key": "alt+x alt+p",
    "command": "markdown.showPreviewToSide",
    "when": "!notebookEditorFocused && editorLangId == 'markdown'"
  },
  {
    "key": "ctrl+k v",
    "command": "-markdown.showPreviewToSide",
    "when": "!notebookEditorFocused && editorLangId == 'markdown'"
  },
  {
    "key": "ctrl+shift+m ctrl+shift+k",
    "command": "markmap-vscode.open"
  },
  {
    "key": "ctrl+, ctrl+k",
    "command": "workbench.action.openGlobalKeybindings"
  },
  {
    "key": "ctrl+k ctrl+s",
    "command": "-workbench.action.openGlobalKeybindings"
  },
  {
    "key": "ctrl+, ctrl+,",
    "command": "workbench.action.openSettings2"
  },
  {
    "key": "ctrl+alt+]",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "ctrl+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "ctrl+alt+b",
    "command": "-workbench.action.toggleAuxiliaryBar"
  },
  {
    "key": "ctrl+\\ ctrl+t",
    "command": "todo-tree-view.focus"
  },
  {
    "key": "alt+x alt+t",
    "command": "cmake.selectLaunchTarget"
  },
  {
    "key": "ctrl+-",
    "command": "-workbench.action.navigateBack",
    "when": "canNavigateBack"
  },
  {
    "key": "alt+left",
    "command": "-workbench.action.navigateBack",
    "when": "canNavigateBack"
  },
  {
    "key": "ctrl+shift+-",
    "command": "-workbench.action.navigateForward",
    "when": "canNavigateForward"
  },
  {
    "key": "ctrl+-",
    "command": "workbench.action.navigateBack"
  },
  {
    "key": "ctrl+shift+numpad_divide",
    "command": "workbench.action.zoomOut"
  },
  {
    "key": "ctrl+-",
    "command": "-workbench.action.zoomOut"
  },
  {
    "key": "shift+alt+-",
    "command": "workbench.action.zoomIn"
  },
  {
    "key": "ctrl+=",
    "command": "-workbench.action.zoomIn"
  },
  {
    "key": "ctrl+shift+alt+enter",
    "command": "editor.action.openLink"
  },
  {
    "key": "ctrl+alt+[",
    "command": "workbench.action.toggleAuxiliaryBar"
  },
  {
    "key": "ctrl+w ctrl+q",
    "command": "workbench.action.closeActiveEditor"
  },
  {
    "key": "ctrl+f4",
    "command": "-workbench.action.closeActiveEditor"
  },
  {
    "key": "ctrl+w ctrl+w",
    "command": "-workbench.action.focusNextGroup",
    "when": "!editorTextFocus && !filesExplorerFocus && !inSearchEditor && !replaceInputBoxFocus && !searchViewletFocus && !terminalFocus && neovim.mode != 'cmdline'"
  },
  {
    "key": "ctrl+w ctrl+w",
    "command": "workbench.action.focusNextGroup"
  },
  {
    "key": "ctrl+shift+win+z",
    "command": "vscode-neovim.stop"
  },
  {
    "key": "ctrl+shift+win+x",
    "command": "vscode-neovim.restart"
  },
  {
    "key": "ctrl+w ctrl+c",
    "command": "edit-csv.edit",
    "when": "resourceExtname == '.csv'"
  },
  {
    "key": "ctrl+k right",
    "command": "-workbench.action.moveActiveEditorGroupRight"
  },
  {
    "key": "ctrl+k left",
    "command": "-workbench.action.moveActiveEditorGroupLeft"
  },
  {
    "key": "ctrl+m ctrl+m",
    "command": "editor.toggleFold",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "ctrl+k ctrl+l",
    "command": "-editor.toggleFold",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "ctrl+shift+alt+win+e",
    "command": "jp-katohirohito-extension-vscode-minmax-in-csv.highlightNumbers"
  },
  {
    "key": "ctrl+shift+e",
    "command": "-jp-katohirohito-extension-vscode-minmax-in-csv.highlightNumbers"
  },
  {
    "key": "ctrl+w ctrl+[",
    "command": "workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "ctrl+alt+right",
    "command": "-workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "ctrl+w ctrl+]",
    "command": "workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "ctrl+alt+left",
    "command": "-workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "ctrl+w ctrl+p",
    "command": "copyFilePath",
    "when": "!editorFocus"
  },
  {
    "key": "shift+alt+c",
    "command": "-copyFilePath",
    "when": "!editorFocus"
  },
  {
    "key": "ctrl+w ctrl+p",
    "command": "copyFilePath",
    "when": "editorFocus"
  },
  {
    "key": "shift+alt+c",
    "command": "-copyFilePath",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+shift+-",
    "command": "-workbench.action.zoomOut"
  },
  {
    "key": "ctrl+shift+-",
    "command": "workbench.action.navigateToLastEditLocation"
  },
  {
    "key": "ctrl+k ctrl+q",
    "command": "-workbench.action.navigateToLastEditLocation"
  },
  {
    "key": "ctrl+w ctrl+n",
    "command": "workbench.action.moveEditorToNewWindow"
  },
  {
    "key": "ctrl+z",
    "command": "-arm-debugger.undoCommand"
  },
  {
    "key": "ctrl+shift+f",
    "command": "-extension.icpp.fire",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+\\ ctrl+c",
    "command": "workbench.panel.chat",
    "when": "workbench.panel.chat.view.copilot.active"
  },
  {
    "key": "ctrl+\\ ctrl+c",
    "command": "workbench.panel.chat",
    "when": "workbench.panel.chat.view.copilot.active"
  },
  {
    "key": "ctrl+\\ ctrl+c",
    "command": "workbench.action.chat.open"
  },
  {
    "key": "ctrl+k ctrl+a",
    "command": "selectby.swapActive"
  },
  {
    "key": "ctrl+k ctrl+a",
    "command": "-keybindings.editor.addKeybinding",
    "when": "inKeybindings && keybindingFocus"
  },
  {
    "key": "ctrl+[",
    "command": "-editor.action.outdentLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+]",
    "command": "editor.action.jumpToBracket",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+\\",
    "command": "-editor.action.jumpToBracket",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+]",
    "command": "-editor.action.indentLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+w ctrl+shift+q",
    "command": "workbench.action.closeOtherEditors"
  }
]
